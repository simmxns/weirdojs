{
  "name": "weirdojs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "ENV_PATH=\".env.development\" node ace serve --watch",
    "build": "node ace build --production",
    "start": "ENV_PATH=\".env.production\" node server.js",
    "lint": "eslint . --ext=.ts",
    "fix": "\"npx prettier --write .\" && \"npm run lint --fix\""
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp"
    ]
  },
  "eslintIgnore": [
    "build"
  ],
  "devDependencies": {
    "@adonisjs/assembler": "5.6.2",
    "@japa/preset-adonis": "1.0.15",
    "@japa/runner": "2.0.7",
    "adonis-preset-ts": "2.1.0",
    "eslint": "8.13.0",
    "eslint-plugin-adonis": "2.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-config-prettier": "8.5.0",
    "prettier": "2.6.2",
    "pino-pretty": "7.6.1",
    "typescript": "~4.6",
    "youch": "3.1.1",
    "youch-terminal": "2.1.3"
  },
  "dependencies": {
    "@adonisjs/core": "5.7.5",
    "@adonisjs/repl": "3.1.10",
    "colors": "1.4.0",
    "mongoose": "6.3.0",
    "proxy-addr": "2.0.7",
    "reflect-metadata": "0.1.13",
    "source-map-support": "0.5.21"
  }
}
